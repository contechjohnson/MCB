{
  "functions": {
    "app/dashboard/page.tsx": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/dashboard",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/weekly-report",
      "schedule": "0 22 * * 4"
    },
    {
      "path": "/api/cron/monthly-report",
      "schedule": "0 14 1 * *"
    },
    {
      "path": "/api/cron/sync-meta-ads",
      "schedule": "0 6 * * *"
    }
  ]
}